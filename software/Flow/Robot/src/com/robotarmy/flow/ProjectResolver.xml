<?xml version="1.0" encoding="UTF-8"?>
<!--
Flow MIME Resolver
-->
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
        <ext name="xml"/>
        <resolver mime="application/x-flow+xml">
            <xml-rule>
                <element name="flow"/>
            </xml-rule>
        </resolver>
    </file>
</MIME-resolver>
